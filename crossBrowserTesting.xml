<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Suite">
    <listeners>
        <listener class-name="utilities.ExtentReportManager" />
    </listeners>
    <test name="chrometest">
        <parameter name="os" value="Windows" />
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="testCases.TC001_SignInTest" />
            <class name="testCases.TC002_SigninDDT" />
            <class name="testCases.TC003_SearchSearvice" />
            <class name="testCases.TC004_ProfilePage" />
            <class name="testCases.TC005_MyRequestPage" />
            <class name="testCases.TC006_PaymentHistoryPage" />
            <class name="testCases.TC007_PaymentPage" />
            <class name="testCases.TC008_ContactUsPage" />
            <class name="testCases.TC012_NearMePage" />            
        </classes>
    </test>
    <test name="edgetest">
        <parameter name="os" value="Windows" />
        <parameter name="browser" value="edge" />
        <classes>
            <class name="testCases.TC001_SignInTest" />
            <class name="testCases.TC002_SigninDDT" />
            <class name="testCases.TC003_SearchSearvice" />
            <class name="testCases.TC004_ProfilePage" />
            <class name="testCases.TC005_MyRequestPage" />
            <class name="testCases.TC006_PaymentHistoryPage" />
            <class name="testCases.TC007_PaymentPage" />
            <class name="testCases.TC008_ContactUsPage" />
            <class name="testCases.TC012_NearMePage" />            
        </classes>
    </test>
</suite>